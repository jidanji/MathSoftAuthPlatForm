<script>
    var a1 = new Vue({
        el: "#app",
        data: {
            fileList: [],
            dddd: 100,
            all: [],
            A1: 0,
            A2: 0,
            A3: 0,
            A4: 0,
            A5:0

        },
        mounted: function () {
            this.getdata();
        },
        methods: {
            success: function (response, file, fileList) {
                console.log(response);
                this.fileList = [];
                if (response.suc) {
                    this.$notify({
                        title: '成功提示',
                        message: '数据处理完毕',
                        type: 'success'
                    });
                    this.getdata();
                    $("#modelLoad").modal('hide');
                    return;
                }
                this.$message.error('处理失败' + response.msg);
                $("#modelLoad").modal('hide');
            },
            progress: function (event, file, fileList) {
                console.log(event.percent);
                $("#modelLoad").modal();
            },
            error: function (err, file, fileList) {
                alert("提交失败");
                $("#modelLoad").modal('hide');
            },
            beforeUpload: function (file) {
                const isJPG = file.type === 'application/vnd.ms-excel' || file.type == "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet";
                if (!isJPG) {
                    this.$message.error('上传必须是Excel文件');
                }
                return isJPG
            },
            getdata: function () {
                var that = this;
                $.ajax({
                    url: "GetData?r=" + Math.random(),
                    data: { draw: -1, start: -1, length: -1 },
                    success: function (data) {
                        if (data.suc) {
                            var arr = data.rows;
                            var a2 = $.map(arr, function (item) {
                                var a1 = datetimeFormate(item.UploadTime);
                                item.ShowTime = a1;
                                return item;
                            });
                            that.all = a2;
                            that.A1 = data.A1;
                            that.A2 = data.A2;
                            that.A3 = data.A3;
                            that.A4 = data.A4;
                            that.A5 = data.A5;
                         

                        }
                    }
                })
            }
        },
        computed: {
            JYT: function () {
                var d1 = $.grep(this.all, function (item) { return item.FileType === "JYT" });
                return d1;
            },
            SYJD: function () {
                var d1 = $.grep(this.all, function (item) { return item.FileType === "SYJD" });
                return d1;
            },
            XSC: function () {
                var d1 = $.grep(this.all, function (item) { return item.FileType === "XXC" });
                return d1;
            },
            CWC: function () {
                var d1 = $.grep(this.all, function (item) { return item.FileType === "CWC" });
                return d1;
            }
        }
    })
</script>

