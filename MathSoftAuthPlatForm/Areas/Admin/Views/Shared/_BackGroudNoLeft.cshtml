<!DOCTYPE html>
<html>
<head>
    <title>石家庄工商职业学院招生综合管理系统</title>


    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <link rel="icon" href="../../favicon.ico">

    <!--Jquery-->
    <script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>

    <!--bootstrap-->
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>

    <!-- Custom styles for this template -->

    <link href="@Url.Content("~/Content/DashBorad/dashboard.css?v=2020-9-29")" rel="stylesheet" />

    <script src="~/Scripts/DataTables-1.10.13/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTables-1.10.13/media/js/dataTables.bootstrap4.min.js"></script>

    <link href="~/Scripts/DataTables-1.10.13/media/css/dataTables.bootstrap4.min.css" rel="stylesheet" />


    <link href="~/Content/bootstrapUpdate.css?v=20200131" rel="stylesheet" />

    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />


    <script src="~/Scripts/jquery-validation-1.15.0/dist/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery-validation-1.15.0/dist/localization/messages_zh.min.js"></script>

    <script src="~/Scripts/layer-v3.0.3/layer/layer.js"></script>

    <script src="~/Scripts/datetime.js?v=2.0"></script>


    <link href="~/Content/WorkFlowStatus.css?id=1000" rel="stylesheet" />


    <script src="~/Scripts/layDate-v5.0.9/laydate/laydate.js?v=1.1"></script>
    <link href="~/Scripts/layDate-v5.0.9/laydate/theme/default/laydate.css" rel="stylesheet" />

    <link href="~/Content/octicons.css" rel="stylesheet" />

    <script src="~/Scripts/respond.min.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>

    <meta name="renderer" content=webkit>
    <link href="~/Scripts/animate.min.css" rel="stylesheet" />

    @RenderSection("scripts", required: false)

    <style>
        .modal-title, .modal-header, .modal-body, .modal-footer {
            border: none;
        }
        .navbar-default {
            background-color: #ffffff !important;
            box-shadow: 0 2px 2px #aaaeb1;
        }
        .selected {
            border-right: 4px solid #0d7de0;
            color: #000;
            background-color: #c4e1ff;
        }

            .selected a {
                color: #000;
                font-size: 15px;
            }


        .nav > li > a {
            position: relative;
            display: block;
            padding: 6px 15px;
        }

        #rightNav > li > a {
            line-height: 31px;
        }

        .navbar-default .navbar-brand {
            color: #108ee9;
        }

        .navbar-brand {
            float: left;
            height: 35px;
            padding: 0;
            font-size: 18px;
            line-height: 35px;
            padding-left: 20px;
        }


        #rightNav > li > a {
            line-height: 35px;
            padding: 0;
            margin: 0 15px;
        }

        .navbar {
       min-height:35px;
            }
        @@font-face {
            font-family: 'FZZZHONGHJW';
            src: url('../../../../fonts/FZZZHONGHJW.TTF') format('truetype');
        }
        @@font-face {
            font-family: 'zsYAHEI';
            src: url('../../../../fonts/zsYAHEI.ttf') format('truetype');
        }
    </style>

    <script>
        $(document).ready(function () {
            $('#@ViewBag.Id').addClass("selected")
        });
    </script>
</head>

<body style="overflow:hidden;position:relative"  class="animate__animated animate__fadeIn">

   

    <div class="container-fluid ">
        <div class="row">
             
            <div id="cc" class="" style="padding:15px; overflow:hidden">
                @RenderBody()
            </div>
        </div>
    </div>

    <div aria-hidden="true" data-backdrop="static" class="modal fade " id="modelLoad" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-sm" role="document">
            <div class="modal-content">
                <div class="modal-body">
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">  <i class="glyphicon glyphicon-remove-circle"></i></button>

                    <form>

                        <div class="form-group form-group-sm">
                            <h5 class="pro-title">正在处理中，请耐心等待......</h5>
                            <div class="progress pro-radius">
                                <div class="progress-bar progress-bar-striped gradient load_bar" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100">

                                </div>
                            </div>

                        </div>



                    </form>
                </div>

            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script>
        $(document).ready(function () {
            var id='@ViewBag.CurrentId';
            if(!id)
            {
                id="11111";
            }
            $("#left_link").find("li").removeClass("active");
            $("#left_link").find("li#" + id).addClass("active");
            $('#cc').height($(window).height() - 80);
            $(window).resize(function () {
                $('#cc').height($(window).height() - 80);
            });
        });
    </script>

</body>

</html>

